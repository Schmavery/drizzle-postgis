{
  "name": "drizzle-postgis",
  "version": "1.0.0",
  "description": "A Drizzle extension/plugin to help work with PostGIS and GeoJSON",
  "keywords": [
    "postgis",
    "geojson",
    "drizzle",
    "orm"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "build:docs": "typedoc --out docs ./src/index.ts --excludeInternal",
    "dev": "tsup --watch",
    "format": "biome check --apply ./src",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.2",
    "@types/node": "^20.11.30",
    "@vitest/coverage-v8": "^1.5.3",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.30.5",
    "postgres": "^3.4.4",
    "semantic-release": "^23.0.6",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.3",
    "vitest": "^1.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "wkx": "^0.5.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "drizzle-kit@0.20.14": "patches/drizzle-kit@0.20.14.patch"
    }
  }
}
